{
  "manifest_version": 3,
  "name": "Anti-Phishing Extension",
  "version": "1.0",
  "description": "Warns users about phishing websites",
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
